public class AddingMatrices{
   public static void main(String args[]){
      int a[][]={{5,3,6},{9,3,1},{4,8,2}};
      int b[][]={{4,8,12},{11,9,5},{7,5,2}};
      int c[][]=new int[3][3];

      for(int i = 0;i<3;i++){
         for(int j = 0;j<3;j++){
            c[i][j] = a[i][j]+b[i][j];
            System.out.print(c[i][j]+" ");
         }
         System.out.println();
      }
   }
}
